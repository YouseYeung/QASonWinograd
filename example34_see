(declare-sort thing)
(declare-sort person)
(declare-const p person)
(declare-const Pillar thing)
(declare-const Stage thing)
(declare-rel between (thing person thing))
(declare-rel notSee (person thing))
(declare-rel notSeeAround (person thing))

(assert (not (= Pillar Stage)))

(assert (forall ((x thing) (y thing)) (=> (not (= x y)) (= (between x p y) (or (notSee p y) (notSeeAround p x))))))

(assert (forall ((x thing) (y thing)) (= (and (= x Pillar) (= y Stage)) (between x p y))))

(assert (forall ((x thing)) (= (or (= x Pillar) (= x Stage)) (or (notSee p x) (notSeeAround p x)))))

(assert (exists ((x thing)) (notSee p x)))
(assert (exists ((y thing)) (notSeeAround p y)))

(check-sat)
(get-model)
