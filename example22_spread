(declare-sort thing)
(declare-sort person)
(declare-const cloth thing)
(declare-const table thing)
(declare-rel spreadSthOnSth (person thing thing))
(declare-rel toProtect (thing))
(declare-rel toDisplay (thing))

(assert (not (= cloth table)))

(assert (forall ((x thing) (y thing)) (=> (not (= x y)) (exists ((z person)) (= (spreadSthOnSth z x y) (or (toProtect y) (toDisplay x)))))))

(assert (forall ((x thing) (y thing)) (= (and (= x cloth) (= y table)) (exists ((z person)) (spreadSthOnSth z x y)))))

(assert (forall ((x thing)) (= (or (= x cloth) (= x table)) (or (toProtect x) (toDisplay x)))))

(assert (exists ((x thing)) (toProtect x)))
(assert (exists ((y thing)) (toDisplay y)))

(check-sat)
(get-model)
