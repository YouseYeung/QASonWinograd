(declare-sort person)
(declare-const Sue person)
(declare-const Sally person)
(declare-rel beatAtArunWithSameSpeed (person person))
(declare-rel goodStart (person))
(declare-rel badStart (person))

(assert (not (= Sue Sally)))

(assert (forall ((x person) (y person)) (=> (not (= x y)) (= (beatAtArunWithSameSpeed x y) (and (goodStart x) (badStart y))))))

(assert (= (goodStart Sue) (not (goodStart Sally))))
(assert (= (badStart Sue) (not (badStart Sally))))

(assert (not (=> (beatAtArunWithSameSpeed Sue Sally) (goodStart Sue))))
;(assert (not (=> (beatAtArunWithSameSpeed Sue Sally) (badStart Sally))))

(check-sat)
(get-model)
