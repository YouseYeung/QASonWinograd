(declare-sort person)
(declare-const John person)
(declare-const Billy person)
(declare-rel notSeeSthWithSbInFront (person person))
(declare-rel tall (person))
(declare-rel short (person))

(assert (not (= John Billy)))

(assert (forall ((x person) (y person)) (=> (not (= x y)) (= (notSeeSthWithSbInFront x y) (and (tall y) (short x))))))

(assert (= (tall John) (not (tall Billy))))
(assert (= (short John) (not (short John))))

;(assert (not (=> (notSeeSthWithSbInFront John Billy) (tall Billy))))
(assert (not (=> (notSeeSthWithSbInFront John Billy) (short John))))
(check-sat)
(get-model)

