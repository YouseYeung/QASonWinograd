(declare-sort thing)
(declare-const sackOfPotatoes thing)
(declare-const bagOfFlour thing)
(declare-rel placeAbove (thing thing))
(declare-rel placeBelow (thing thing))
(declare-rel beMovedFirst (thing))

(assert (not (= sackOfPotatoes bagOfFlour)))

(assert (forall ((x thing)) (exists ((y thing)) (=> (not (= x y)) (= (placeAbove x y) (beMovedFirst x))))))

;(assert (forall ((x thing)) (exists ((y thing)) (=> (not (= x y)) (= (placeBelow x y) (beMovedFirst y))))))

(assert (forall ((x thing) (y thing)) (= (and (= x sackOfPotatoes) (= y bagOfFlour)) (placeAbove x y))))

(assert (forall ((x thing)) (= (or (= x sackOfPotatoes) (= x bagOfFlour)) (beMovedFirst x))))

(assert (exists ((x thing)) (beMovedFirst x)))

(check-sat)
(get-model)
