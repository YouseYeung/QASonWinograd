(declare-sort thing)
(declare-sort person)
(declare-const cloth thing)
(declare-const table thing)
(declare-const I person)
(declare-rel spreadSthOnSth (person thing thing))
(declare-rel display (thing))
(declare-rel protect (thing))
(declare-rel cover (thing))

(assert (not (= cloth table)))

;(assert (forall ((x thing) (y thing)) (=> (not (= x y)) (= (spreadSthOnSth I x y) (display x)))))
(assert (forall ((x thing) (y thing)) (=> (not (= x y)) (= (spreadSthOnSth I x y) (cover y)))))
(assert (forall ((x thing)) (= (cover x) (protect x))))

(assert (= (display cloth) (not (display table))))
(assert (= (protect cloth) (not (protect table))))

;(assert (not (=> (spreadSthOnSth I cloth table) (display cloth))))
(assert (not (=> (spreadSthOnSth I cloth table) (protect table))))

(check-sat)
(get-model)