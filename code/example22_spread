(declare-sort thing)
(declare-sort person)
(declare-const cloth thing)
(declare-const table thing)
(declare-const I person)
(declare-rel spreadSthOnSth (person thing thing))
(declare-rel display (thing))
(declare-rel protect (thing))

(assert (not (= cloth table)))

(assert (forall ((x thing) (y thing)) (=> (not (= x y)) (=> (spreadSthOnSth I x y) (and (protect y) (display x))))))

(assert (forall ((x thing) (y thing)) (= (and (= x cloth) (= y table)) (spreadSthOnSth I x y))))

(assert (or (and (display cloth) (not (display table))) (and (display table) (not (display cloth)))))
(assert (or (and (protect cloth) (not (protect table))) (and (protect table) (not (protect cloth)))))

(assert (not (=> (spreadSthOnSth I cloth table) (display table))))
;(assert (not (=> (spreadSthOnSth I cloth table) (protect table))))

(check-sat)