(declare-sort person)
(declare-const conArtist person)
(declare-const Sam person)
(declare-rel succeedInFooling (person person))
(declare-rel getMoney (person))
(declare-rel loseMoney (person))


(assert (not (= conArtist Sam)))

(assert (forall ((x person) (y person)) (= (and (= x conArtist) (= y Sam)) (succeedInFooling x y))))

(assert (forall ((x person) (y person)) (=> (not (= x y)) (= (succeedInFooling x y) (or (getMoney x) (loseMoney y))))))


(assert (forall ((x person)) (= (or (= x conArtist) (= x Sam)) (or (loseMoney x) (getMoney x)))))

(assert (exists ((x person)) (getMoney x)))
(assert (exists ((y person)) (loseMoney y)))
(check-sat)
(get-model)
