(declare-sort thing)
(declare-sort person)
(declare-const p person)
(declare-const bottle thing)
(declare-const table thing)
(declare-rel balanceSthOnSth (person thing thing))
(declare-rel top-heavy (thing))
(declare-rel uneven (thing))

(assert (not (= bottle table)))

(assert (forall ((x thing) (y thing)) (=> (not (= x y)) (=> (balanceSthOnSth p x y) (and (top-heavy x) (uneven y))))))

(assert (forall ((x thing) (y thing)) (= (and (= x bottle) (= y table)) (balanceSthOnSth p x y))))

(assert (or (and (top-heavy bottle) (not (top-heavy table))) (and (top-heavy table) (not (top-heavy bottle)))))
(assert (or (and (uneven bottle) (not (uneven table))) (and (uneven table) (not (uneven bottle)))))

(assert (not (=> (balanceSthOnSth p bottle table) (top-heavy bottle))))
;(assert (not (=> (balanceSthOnSth p bottle table) (uneven table))))

(check-sat)
(get-model)
