(declare-sort person)
(declare-const Bob person)
(declare-const aMan person)
(declare-rel seeDoingSth (person person))
(declare-rel impressive (person))
(declare-rel impressed (person))


(assert (not (= Bob aMan)))

(assert (forall ((x person) (y person)) (= (and (= x Bob) (= y aMan)) (seeDoingSth x y))))

(assert (forall ((x person) (y person)) (=> (not (= x y)) (= (seeDoingSth x y) (or (impressive y) (impressed x))))))


(assert (forall ((x person)) (= (or (= x Bob) (= x aMan)) (or (impressed x) (impressive x)))))

(assert (exists ((x person)) (impressive x)))
(assert (exists ((y person)) (impressed y)))
(check-sat)
(get-model)
