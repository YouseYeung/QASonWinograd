(declare-sort thing)
(declare-const key thing)
(declare-const chewingGum thing)
(declare-rel openLockWith (thing))
(declare-rel fillTheKeyhole (thing))
(declare-rel getIn (thing))
(declare-rel getOut (thing))

(assert (not (= chewingGum key)))

(assert (forall ((x thing)) (= (= x key) (openLockWith x))))
(assert (forall ((x thing)) (= (= x chewingGum) (fillTheKeyhole x))))
(assert (forall ((x thing)) (= (openLockWith x) (getIn x))))
(assert (forall ((x thing)) (= (fillTheKeyhole x) (getOut x))))

(assert (forall ((x thing)) (= (or (= x key) (= x chewingGum)) (or (getIn x) (getOut x)))))

(assert (exists ((x thing)) (getIn x)))
(assert (exists ((y thing)) (getOut y)))
(check-sat)
(get-model)
