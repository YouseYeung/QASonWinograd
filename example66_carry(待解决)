(declare-sort thing)
(declare-sort person)
(declare-const p person)
(declare-const newspaper thing)
(declare-const backpack thing)
(declare-rel carrySthOverSth (person thing thing))
(declare-rel carrySthInSth (person thing thing))
(declare-rel over (thing thing))
(declare-rel dry (thing))

(assert (not (= newspaper backpack)))

(assert (forall ((x thing) (y thing)) (=> (not (= x y)) (= (or (carrySthOverSth p y x) (carrySthInSth p x y)) (over y x)))))

(assert (forall ((x thing) (y thing)) (=> (not (= x y)) (= (over x y) (or (not (dry x)) (dry y))))))

;(assert (forall ((x thing) (y thing)) (= (and (= x newspaper) (= y backpack)) (and (not (carrySthInSth p x y)) (carrySthOverSth p x y)))))

(assert (forall ((x thing) (y thing)) (= (and (= x newspaper) (= y backpack)) (and (not (carrySthOverSth p x y)) (carrySthInSth p x y)))))

(assert (forall ((x thing)) (= (or (= x newspaper) (= x backpack)) (dry x))))

(assert (exists ((x thing)) (dry x)))

(check-sat)
(get-model)
