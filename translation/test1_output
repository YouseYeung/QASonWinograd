(declare-sort thing)
(declare-const trophy thing)
(declare-const suitcase thing)
(declare-const it thing)
(declare-sort person)
(declare-rel small (thing ))
(declare-rel big (thing ))
(declare-rel fit (thing thing ))
(assert (not (= trophy suitcase)))
(assert (not (= trophy it)))
(assert (not (= suitcase it)))
(assert (forall ((x thing)) (or (= x trophy) (= x suitcase) (= x it) )))
(assert (forall ((a thing) (b thing) ) (=> ( not (= a b) ) (=> (and (big a ) (small b ) ) (fit a b ) ))))
(assert (forall ((a thing) (b thing) ) (=> ( not (= a b) ) (=> (fit a b ) (and (big a ) (small b ) ) ))))
(assert (= (small trophy) (and (not (small suitcase)) (not (small it)) )))
(assert (= (small suitcase) (and (not (small trophy)) (not (small it)) )))
(assert (= (small it) (and (not (small trophy)) (not (small suitcase)) )))
(assert (fit trophy suitcase ))
(assert (not (small it)))
(check-sat)
