(declare-sort thing)
(declare-const violence thing)
(declare-sort person)
(declare-const councilmen person)
(declare-const demonstrator person)
(declare-const they person)
(declare-rel fear (person thing ))
(declare-rel advocate (person thing ))
(declare-rel refuse (person person thing ))
(assert (not (= councilmen demonstrator))
(assert (not (= councilmen they))
(assert (not (= demonstrator they))
(assert (forall ((x thing)) (= x violence)))
(assert (forall ((x person)) (or (= x councilmen) (= x demonstrator) (= x they) )))
(assert (forall ((a person)(b person))