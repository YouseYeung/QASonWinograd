(declare-sort thing)
(declare-const city thing)
(declare-const councilmen thing)
(declare-const permit thing)
(declare-const violence thing)
(assert (not (= city councilmen))
(assert (not (= city permit))
(assert (not (= city violence))
(assert (forall ((x thing)) (or (= x city) (= x councilmen) (= x permit) (= x violence) )))
