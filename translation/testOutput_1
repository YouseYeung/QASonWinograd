(declare-sort thing)
(declare-const permit thing)
(declare-const violence thing)
(declare-sort person)
(declare-const councilmen person)
(declare-const demonstrator person)
(declare-const they person)
(declare-rel fear (person thing ))
(declare-rel advocate (person thing ))
(declare-rel refuse (person person thing ))
(assert (not (= permit violence)))
(assert (not (= councilmen demonstrator)))
(assert (not (= councilmen they)))
(assert (not (= demonstrator they)))
(assert (forall ((x thing)) (or (= x permit) (= x violence) )))
(assert (forall ((x person)) (or (= x councilmen) (= x demonstrator) (= x they) )))
(assert (forall ((a person) (b person) ) (=> (not (= a b) ) (=> (and (fear a violence) (advocate b violence) ) (refuse a b permit) ))))
(assert (forall ((a person) (b person) ) (=> (not (= a b) ) (=> (refuse a b permit) (and (fear a violence) (advocate b violence) ) ))))
(assert (= (fear councilmen violence) (and (not (fear demonstrator violence)) (not (fear they violence)) )))
(assert (= (fear demonstrator violence) (and (not (fear councilmen violence)) (not (fear they violence)) )))
(assert (= (fear they violence) (and (not (fear councilmen violence)) (not (fear demonstrator violence)) )))
(assert (refuse councilmen demonstrator permit ))
(assert (not (fear councilmen violence)))
(check-sat)
