(declare-sort thing)
(declare-sort person)
(declare-const Noun_coat thing)
(declare-const Noun_cold thing)
(declare-const Noun_they thing)
(declare-const Noun_person thing)
(declare-const Noun_Fred thing)
(declare-const Noun_Alice thing)
(assert (not (= Noun_coat Noun_cold)))
(assert (not (= Noun_coat Noun_they)))
(assert (not (= Noun_coat Noun_person)))
(assert (not (= Noun_coat Noun_Fred)))
(assert (not (= Noun_coat Noun_Alice)))
(assert (not (= Noun_cold Noun_they)))
(assert (not (= Noun_cold Noun_person)))
(assert (not (= Noun_cold Noun_Fred)))
(assert (not (= Noun_cold Noun_Alice)))
(assert (not (= Noun_they Noun_person)))
(assert (not (= Noun_they Noun_Fred)))
(assert (not (= Noun_they Noun_Alice)))
(assert (not (= Noun_person Noun_Fred)))
(assert (not (= Noun_person Noun_Alice)))
(assert (not (= Noun_Fred Noun_Alice)))
(assert (forall ((x thing)) (or (= x Noun_coat) (= x Noun_cold) (= x Noun_they) (= x Noun_person) (= x Noun_Fred) (= x Noun_Alice) )))
(declare-rel Verb_not_enough_for (thing thing ))
(declare-rel Verb_enough_1 (thing ))
(declare-rel Verb_enough_2 (thing thing ))
(declare-rel Verb_enough (thing thing ))
(declare-rel Verb_not_prepare_for (thing thing ))
(declare-rel Verb_prepare_1 (thing ))
(declare-rel Verb_prepare_2 (thing thing ))
(declare-rel Verb_prepare (thing thing ))
(declare-rel Verb_person (thing ))
(declare-rel Verb_not_person (thing ))
(assert (Verb_not_enough_for Noun_coat Noun_cold ))
(assert (= (Verb_enough_for Noun_coat Noun_cold ) (not (Verb_not_enough_for Noun_coat Noun_cold ))))
(assert (Verb_not_prepare_for Noun_person Noun_cold ))
(assert (= (Verb_prepare_for Noun_person Noun_cold ) (not (Verb_not_prepare_for Noun_person Noun_cold ))))
(assert (Verb_person Noun_Fred ))
(assert (= (Verb_person Noun_Fred ) (not (Verb_not_person Noun_Fred ))))
(assert (Verb_person Noun_Alice ))
(assert (= (Verb_person Noun_Alice ) (not (Verb_not_person Noun_Alice ))))
(assert (=> (Verb_not_enough_for Noun_coat Noun_cold ) (Verb_enough_1 Noun_coat )))
(assert (= (Verb_not_enough_for Noun_coat Noun_cold ) (Verb_enough_2 Noun_coat Noun_cold )))
(assert (=> (Verb_not_prepare_for Noun_person Noun_cold ) (Verb_prepare_1 Noun_person )))
(assert (= (Verb_not_prepare_for Noun_person Noun_cold ) (Verb_prepare_2 Noun_person Noun_cold )))
(assert (not  (Verb_enough_2 Noun_Alice Noun_cold )))
(check-sat)
