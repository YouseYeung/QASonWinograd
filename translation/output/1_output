(declare-sort thing)
(declare-sort person)
(declare-const Noun_permit thing)
(declare-const Noun_violence thing)
(declare-const Noun_city_councilmen person)
(declare-const Noun_demonstrator person)
(declare-const Noun_they person)
(assert (not (= Noun_permit Noun_violence)))
(assert (not (= Noun_city_councilmen Noun_demonstrator)))
(assert (not (= Noun_city_councilmen Noun_they)))
(assert (not (= Noun_demonstrator Noun_they)))
(assert (forall ((x thing)) (or (= x Noun_permit) (= x Noun_violence) )))
(assert (forall ((x person)) (or (= x Noun_city_councilmen) (= x Noun_demonstrator) (= x Noun_they) )))
(declare-rel Verb_refuse (person person thing ))
(declare-rel Verb_refuse_1 (person ))
(declare-rel Verb_refuse_2 (person person ))
(declare-rel Verb_refuse_3 (person person thing ))
(declare-rel Verb_not_refuse (person person thing ))
(declare-rel Verb_advocate (person thing ))
(declare-rel Verb_advocate_1 (person ))
(declare-rel Verb_advocate_2 (person thing ))
(declare-rel Verb_not_advocate (person thing ))
(declare-rel Verb_fear (person thing ))
(declare-rel Verb_fear_1 (person ))
(declare-rel Verb_fear_2 (person thing ))
(declare-rel Verb_not_fear (person thing ))
(assert (forall ((a person) (b person) ) (=> (not (= a b)) (= (and (Verb_fear a Noun_violence) (Verb_advocate b Noun_violence))  (Verb_refuse a b Noun_permit) ))))
(assert (forall ((b person) (c person) ) (= (Verb_refuse b c Noun_permit ) (not (Verb_not_refuse b c Noun_permit )))))
(assert (forall ((b person) ) (= (Verb_advocate b Noun_violence ) (not (Verb_not_advocate b Noun_violence )))))
(assert (forall ((b person) ) (= (Verb_fear b Noun_violence ) (not (Verb_not_fear b Noun_violence )))))
(assert (forall ((b person) (c person) ) (=> (Verb_refuse b c Noun_permit ) (Verb_refuse_1 b ))))
(assert (forall ((b person) (c person) ) (=> (Verb_refuse b c Noun_permit ) (Verb_refuse_2 b c ))))
(assert (forall ((b person) (c person) ) (= (Verb_refuse b c Noun_permit ) (Verb_refuse_3 b c Noun_permit ))))
(assert (forall ((b person) ) (=> (Verb_advocate b Noun_violence ) (Verb_advocate_1 b ))))
(assert (forall ((b person) ) (= (Verb_advocate b Noun_violence ) (Verb_advocate_2 b Noun_violence ))))
(assert (forall ((b person) ) (=> (Verb_fear b Noun_violence ) (Verb_fear_1 b ))))
(assert (forall ((b person) ) (= (Verb_fear b Noun_violence ) (Verb_fear_2 b Noun_violence ))))
(assert (= (Verb_fear Noun_city_councilmen Noun_violence ) (not (Verb_fear Noun_demonstrator Noun_violence )) ))
(assert (= (Verb_fear Noun_demonstrator Noun_violence ) (not (Verb_fear Noun_city_councilmen Noun_violence )) ))
(assert (Verb_refuse Noun_city_councilmen Noun_demonstrator Noun_permit ) )
(assert (not  (Verb_fear Noun_city_councilmen Noun_violence )))
(check-sat)
