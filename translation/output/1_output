(declare-sort thing)
(declare-sort person)
(declare-const Noun_question thing)
(declare-const Noun_it thing)
(declare-const Noun_lawyer person)
(declare-const Noun_witness person)
(declare-const Noun_he person)
(assert (not (= Noun_question Noun_it)))
(assert (not (= Noun_lawyer Noun_witness)))
(assert (not (= Noun_lawyer Noun_he)))
(assert (not (= Noun_witness Noun_he)))
(assert (forall ((x thing)) (or (= x Noun_question) (= x Noun_it) )))
(assert (forall ((x person)) (or (= x Noun_lawyer) (= x Noun_witness) (= x Noun_he) )))
(declare-rel Verb_not_answer (person thing ))
(declare-rel Verb_answer_1 (person ))
(declare-rel Verb_answer_2 (person thing ))
(declare-rel Verb_answer (person thing ))
(declare-rel Verb_ask (person person thing ))
(declare-rel Verb_not__repeat (person thing ))
(declare-rel Verb__repeat_1 (person ))
(declare-rel Verb__repeat_2 (person thing ))
(declare-rel Verb__repeat (person thing ))
(assert (forall ((a person) (b person) ) (=> (not (= a b)) (=> (Verb_ask b a Noun_question)  (and (Verb_not__repeat b Noun_question) (Verb_not_answer a Noun_question)) ))))
(assert (forall ((a person) ) (= (Verb_answer a Noun_question) (not (Verb_not_answer a Noun_question)))))
(assert (forall ((a person) ) (= (Verb__repeat a Noun_question) (not (Verb_not__repeat a Noun_question)))))
(assert (forall ((b person) ) (=> (Verb_not_answer b Noun_question ) (Verb_answer_1 b ))))
(assert (forall ((b person) ) (= (Verb_not_answer b Noun_question ) (Verb_answer_2 b Noun_question ))))
(assert (forall ((b person) ) (=> (Verb_not__repeat b Noun_question ) (Verb__repeat_1 b ))))
(assert (forall ((b person) ) (= (Verb_not__repeat b Noun_question ) (Verb__repeat_2 b Noun_question ))))
(assert (= (Verb_answer Noun_lawyer Noun_question ) (not (Verb_answer Noun_witness Noun_question )) ))
(assert (= (Verb_answer Noun_witness Noun_question ) (not (Verb_answer Noun_lawyer Noun_question )) ))
(assert (Verb_ask Noun_lawyer Noun_witness Noun_question ) )
(assert (not  (Verb_answer Noun_lawyer Noun_question )))
(check-sat)
