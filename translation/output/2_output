(declare-sort thing)
(declare-sort person)
(declare-const Noun_she person)
(declare-const Noun_Susan person)
(declare-const Noun_it thing)
(assert (not (= Noun_she Noun_Susan)))
(assert (forall ((x person)) (or (= x Noun_she) (= x Noun_Susan) )))
(assert (forall ((x thing)) (= x Noun_it)))
(declare-rel Verb_tell_about (person person thing ))
(declare-rel Verb_tell_1 (person ))
(declare-rel Verb_tell_2 (person person ))
(declare-rel Verb_tell_3 (person person thing ))
(declare-rel Verb_know (person thing ))
(declare-rel Verb_tell (person person thing ))
(declare-rel Verb_possess_pt (person thing))
(declare-rel Verb_possess_tt (thing thing))
(assert (forall ((a person) ) (exists ((b person) (c thing)) (=> (not (= a b)) (=> (Verb_know a c)  (Verb_tell_about a b c) ))))
(assert (forall ((a person) (b person) ) (exists ((c thing)) (=> (not (= a b)) (= (Verb_tell a b c)  (Verb_know b c) ))))
(assert (forall ((b person) (c person) (d thing) ) (=> (Verb_tell_about b c d ) (Verb_tell_1 b ))))
(assert (forall ((b person) (c person) (d thing) ) (=> (Verb_tell_about b c d ) (Verb_tell_2 b c ))))
(assert (forall ((b person) (c person) (d thing) ) (= (Verb_tell_about b c d ) (Verb_tell_3 b c d ))))
(assert (Verb_know Noun_Susan ) )
(assert (not  (Verb_tell_2 Noun_Susan Noun_accident )))
(check-sat)
