(declare-sort thing)
(declare-sort person)
(declare-const Noun_question thing)
(declare-const Noun_it thing)
(declare-const Noun_lawyer person)
(declare-const Noun_witness person)
(declare-const Noun_he person)
(assert (not (= Noun_question Noun_it)))
(assert (not (= Noun_lawyer Noun_witness)))
(assert (not (= Noun_lawyer Noun_he)))
(assert (not (= Noun_witness Noun_he)))
(assert (forall ((x thing)) (or (= x Noun_question) (= x Noun_it) )))
(assert (forall ((x person)) (or (= x Noun_lawyer) (= x Noun_witness) (= x Noun_he) )))
(declare-rel Verb__repeat (person thing ))
(declare-rel Verb__repeat_1 (person ))
(declare-rel Verb__repeat_2 (person thing ))
(declare-rel Verb_not__repeat (person thing ))
(declare-rel Verb_ask (person person thing ))
(declare-rel Verb_ask_1 (person ))
(declare-rel Verb_ask_2 (person person ))
(declare-rel Verb_ask_3 (person person thing ))
(declare-rel Verb_not_ask (person person thing ))
(declare-rel Verb_reluctant (person ))
(declare-rel Verb_reluctant_1 (person ))
(declare-rel Verb_not_reluctant (person ))
(assert (forall ((a person) (b person) ) (=> (not (= a b)) (=> (Verb_ask a b Noun_question)  (and (Verb_reluctant a) (Verb__repeat a Noun_question) )))))
(assert (forall ((b person) ) (= (Verb__repeat b Noun_question ) (not (Verb_not__repeat b Noun_question )))))
(assert (forall ((b person) (c person) ) (= (Verb_ask b c Noun_question ) (not (Verb_not_ask b c Noun_question )))))
(assert (forall ((b person) ) (= (Verb_reluctant b ) (not (Verb_not_reluctant b )))))
(assert (forall ((b person) ) (=> (Verb__repeat b Noun_question ) (Verb__repeat_1 b ))))
(assert (forall ((b person) ) (= (Verb__repeat b Noun_question ) (Verb__repeat_2 b Noun_question ))))
(assert (forall ((b person) (c person) ) (=> (Verb_ask b c Noun_question ) (Verb_ask_1 b ))))
(assert (forall ((b person) (c person) ) (=> (Verb_ask b c Noun_question ) (Verb_ask_2 b c ))))
(assert (forall ((b person) (c person) ) (= (Verb_ask b c Noun_question ) (Verb_ask_3 b c Noun_question ))))
(assert (forall ((b person) ) (= (Verb_reluctant b ) (Verb_reluctant_1 b ))))
(assert (Verb_ask Noun_lawyer Noun_witness Noun_question ) )
(assert (not (and  (Verb_reluctant Noun_lawyer ) (Verb__repeat Noun_lawyer Noun_question ))))
(check-sat)
